    "def fun(arr):\n",
    "    maxl=0\n",
    "    for i in range(len(arr)):\n",
    "        cur=0\n",
    "        for j in range(i,len(arr)):\n",
    "            cur+=arr[j]\n",
    "            if cur==0:\n",
    "                maxl=max(maxl,j-i+1)\n",
    "    return maxl\n",
    "arr=[9,-3,3,-1,6,-5]\n",
    "res=fun(arr)\n",
    "print(res)"
   ]
