      "source": [
        "arr= [1,3,5,2,7,4,8,9,5,3,5,7,9,3,2,0]\n",
        "n=int(input(\"Enter the consecutive number\"))\n",
        "l=len(arr)\n",
        "sum=0\n",
        "for i in range(0,n):\n",
        "    sum+=arr[i]\n",
        "win = sum\n",
        "for i in range(0,l-n):\n",
        "    if(win>sum):\n",
        "        sum = win\n",
        "    win-=arr[i]+arr[i+n]\n",
        "print(sum)"
      ]
